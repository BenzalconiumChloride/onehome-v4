{
  "expo": {
    "name": "onehome-v3",
    "slug": "onehome-v3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/silverlogo.png",
    "scheme": "onehome",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/silverlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./assets/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/silverlogo.png",
        "backgroundColor": "#ffffff",
        "package": "com.benzalconium.onehomev3"
      },
      "package": "com.benzalconium.onehomev3"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
    },
    "plugins": [
      "expo-camera",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
